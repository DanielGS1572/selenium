package Section9_Synchronization;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class A4_Actions {
/*A veces es necesario capturar algunas de las acciones, por ejemplo en amazon
 * si se coloca el cursos en el login, no es necesario dar clic, simplemente se abre
 */
	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver",
				"C:\\Users\\Danielgonzali\\Documents\\Selenium\\repo\\selenium\\Artefactos\\chromedriver74.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("");
		driver.manage().window().maximize();

	}

}
