package demos;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Locators {
/*Existen varias formas de encontrar un elemento:
 * - ID
 * - ClassName
 * - Name
 * - linkText
 * Si no se tienen la forma de acceder con los datos anteriores puede ser a traves de Xpath ó CSS
 * - Xpath
 * - Css
	*/
	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\Danielgonzali\\Documents\\Selenium\\chromedriver74.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("http://yahoo.com");
		driver.findElement(By.id("uh-search-box")).sendKeys("prueba");
	}

}
